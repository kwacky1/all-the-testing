	if ([string]::IsNullOrEmpty($YAxisLabel))
	{
		$displayYAxisLabel = 'false'
	}
	else 
	{
		$displayYAxisLabel = 'true'
	}

	$CJSOptions += "scales: {
                        xAxes: [{
                            display: $displayXAxisLabel,
                            scaleLabel: {
                                display: $displayXAxisLabel,
                                labelString: '$XAxisLabel'
                            }
                        }],
